<UserControl x:Class="AGCSE.ActiveGanttCSECtl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">
    <Canvas x:Name="oCanvas" Background="White" Loaded="oCanvas_Loaded" KeyDown="oCanvas_KeyDown" KeyUp="oCanvas_KeyUp" MouseLeave="oCanvas_MouseLeave" MouseLeftButtonDown="oCanvas_MouseLeftButtonDown" MouseRightButtonDown="oCanvas_MouseRightButtonDown" MouseMove="oCanvas_MouseMove" MouseLeftButtonUp="oCanvas_MouseLeftButtonUp" MouseRightButtonUp="oCanvas_MouseRightButtonUp" MouseWheel="oCanvas_MouseWheel">
        <ContentControl IsTabStop="True">
            <Image Name="oImage" />
        </ContentControl>
    </Canvas>
</UserControl>
